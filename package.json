{
  "name": "bloc-pika",
  "displayName": "Bloc Pika",
  "description": "Generation bloc file from events file",
  "version": "0.5.5",
  "license": "MIT",
  "publisher": "TenraNeko",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:bloc-pika.generate",
    "onCommand:bloc-pika.createBloc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bloc-pika.createBloc",
        "title": "Bloc Pika: Create Bloc"
      },
      {
        "command": "bloc-pika.generate",
        "title": "Bloc Pika: Bloc from Events"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "bloc-pika.createBloc",
          "when": "filesExplorerFocus",
          "group": "bloc-pika@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "homepage": "https://github.com/tenraneko/vscode_event_to_bloc_extention",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenraneko/vscode_event_to_bloc_extention.git"
  }
}
