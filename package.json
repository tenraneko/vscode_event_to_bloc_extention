{
  "name": "generate-bloc-from-events",
  "displayName": "Generate Bloc from Events",
  "description": "Generation bloc file from events file",
  "version": "0.2.3",
  "license": "MIT",
  "publisher": "TenraNeko",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDebug",
    "onCommand:generate-bloc-from-events.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "generate-bloc-from-events.generate",
        "title": "Generate: Bloc from Events"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "homepage": "https://github.com/tenraneko/vscode_event_to_bloc_extention",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenraneko/vscode_event_to_bloc_extention.git"
  }
}
